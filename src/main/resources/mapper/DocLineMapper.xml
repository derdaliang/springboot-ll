<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Mapper.DocLineMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.example.demo.pojo.DocLine">
                    <id column="id" property="id" />
                    <result column="goods_id" property="goodsId" />
                    <result column="ma_no" property="maNo" />
                    <result column="mt_area" property="mtArea" />
                    <result column="lot_no" property="lotNo" />
                    <result column="mt_qty" property="mtQty" />
                    <result column="date" property="date" />
                    <result column="time" property="time" />
                    <result column="user_no" property="userNo" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
        id, goods_id, ma_no, mt_area, lot_no, mt_qty, date, time, user_no
    </sql>

</mapper>
